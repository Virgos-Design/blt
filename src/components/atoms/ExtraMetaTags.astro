---
import { SITE } from '~/config.mjs';
---

<!-- astrowind -->
<link rel="shortcut icon" sizes="any" href={`${SITE.basePathname}favicon.ico`} />
<link rel="icon" type="image/svg+xml" href={`${SITE.basePathname}favicon.svg`} />
<link rel="mask-icon" href={`${SITE.basePathname}favicon.svg`} color="#8D46E7" />
<!-- virgos -->
<link rel="apple-touch-icon" sizes="180x180" href={`${SITE.basePathname}apple-touch-icon.png`} />
<link rel="icon" type="image/png" sizes="32x32" href={`${SITE.basePathname}favicon-32x32.png`} />
<link rel="icon" type="image/png" sizes="16x16" href={`${SITE.basePathname}favicon-16x16.png`} />
<link rel="manifest" href={`${SITE.basePathname}site.webmanifest`} />
<link rel="mask-icon" href={`${SITE.basePathname}safari-pinned-tab.svg?v=Xa23fdd`} color="#321A81" />
<meta name="msapplication-TileColor" content="#da532c" />
<meta name="theme-color" content="#ffffff" />
